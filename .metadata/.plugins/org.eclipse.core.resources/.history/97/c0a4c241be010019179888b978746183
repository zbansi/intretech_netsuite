/**
 * @NApiVersion 2.x
 * @NScriptType UserEvent
 * @author YQ12681
 */

define([ '' ], function() {
	beforeSubmit(context)// context.record,type,form,
	{ var status = context.form.getField({id: 'custrecord_yq_workorder_status_bs'});
	if(status =='NEW')
		context.form.addButton({
		id:'',
		label:''
	});

	}

	return {
		beforeLoad : beforeLoad
	};
});