/**
 * @NApiVersion 2.x
 * @author YQ12681
 */
define([ 'N/url', 'N/currentRecord' ], function(url, cr) {
	return ({
		create_mao_url : function () {
			try {
				var u = url.resolveRecord({
					recordType : '9',					
					recordId : 8,
					isEditMode : true
				});
				log.debug({
					title : 'Success',
					details : 'url创建成功'
				});
			} catch (e1) {
				log.debug({
					title : e1.name,
					details : e1.message
				});
			}
			try {
				var woid = cr.CurrentRecord.id;
				log.debug({
					title : 'Success',
					details : '获取记录id =' + woid
				});
			} catch (e2) {
				log.debug({
					title : e2.name,
					details : e2.message
				});
			}
			window.location.href = u + '&woid=' + woid;
		}
	});
});