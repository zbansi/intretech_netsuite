/**
 * @NApiVersion 2.x
 * @NScriptType clientScript
 * @author YQ12681 Bansi Description 当领料单打印页面字段'状态'变更时，为url添加参数
 */

define([ 'N/http' ], function() {
	function fieldChanged(type, name, linenum) {
		try {
			if (name == 'custpage_status_bs') {
				var url = window.location.href;
				url += '&status=' + nlapiGetFieldValue('custpage_status');
				window.location.href = url;
			}
			log.debug({
				title : 'Success',
				details : 'url重定向成功'
			})
		} catch (ex) {
			log.debug({
				title : ex.name,
				details : ex.message
			})
		}
	}
	return {
		filedChanged : filedChanged
	};
});