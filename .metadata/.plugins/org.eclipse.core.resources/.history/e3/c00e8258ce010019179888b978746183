/**
 * @NApiVersion 2.x
 * @author YQ12681
 */
require([ 'N/url', 'N/currentRecord' ], function(url, cr) {
	return ({
		create_mao_url : function createMAO() {
			try {
				var url = resolveRecord({
					recordType : 'customrecord_yq_material_apply_bansi',
					recordId : '9',
					isEditMode : true
				});
			} catch (e1) {
				log.debug({
					title : e1.name,
					details : e1.message
				});
			}
			try {
				var woid = cr.CurrentRecord.id;
			} catch (e2) {
				log.debug({
					title : e2.name,
					details : e2.message
				});
			}
			window.location.href = url + '&woid=' + woid;
		}
	});
});