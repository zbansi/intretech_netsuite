{
	"baseUrl" : "./oauth_valid", // 设置站点的静态文件根目录
	// 将一些常用的模块的路径映射为别名
	// 以 key value 的形势组合
	// 在加载模块该模块时, 只需要传入模块的别名就能找到完整的路径
	"paths" : {
		"oauth" : "./oauth_valid"

	},
	// 配置不兼容的模块

	"shim" : {
		"oauth-1.0a" : {// 这个键名为要载入的目标文件的文件名，不能随便命名。

			// 下述依赖脚本应在oauth-1.0a.js之前加载
			// deps: [ '' ],
			// 一旦加载，使用全局变量'OAuth'作为模块值
			exports : "OAuth"// exports的值为oauth.js提供的 对外接口的名称 
				}

		}

	}