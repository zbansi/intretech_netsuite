define(['N/record'],function(record){
	function onRequest(params){
		var maoId = params.request.parameters.maoid;
		var rec = 	record.load({
			type : 'customrecord_yq_material_apply_bansi',
			id : maoId
		});
	}
	
	
	return {
		onRequest : onRequest
	};
});