/**
 * @NApiVersion 2.x
 * @NScriptType MassUpdateScript
 * @author YQ12681
 */
define([ 'N/record' ],
/**
 * @param {record} record
 */
function(record) {
	function each(params) {
		var maoRecord = record.load({ type : params.type,
		id : params.id });
		maoRecord.setValue({});
		maoRecord.save();
	}
	return { each : each };
})