package com.netsuite.suitetalk.client.common.contract;

import com.netsuite.suitetalk.client.common.authentication.*;

public interface Authentication
{
    Passport getPassport();
    
    void setPassport(final Passport p0);
    
    void unsetPassport();
    
    void setRequestLevelCredentials(final boolean p0);
    
    SsoPassport getSsoPassport();
    
    void setSsoPassport(final SsoPassport p0);
    
    void unsetSsoPassport();
    
    TokenPassport getTokenPassport();
    
    void setTokenPassport(final TokenPassport p0);
    
    void unsetTokenPassport();
    
    OAuthPassport getOAuthPassport();
    
    void setOAuthPassport(final OAuthPassport p0);
    
    void unsetOAuthPassport();
}
